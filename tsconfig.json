{
    "compilerOptions": {
        "skipLibCheck": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "strict": true,
        "noImplicitAny": false,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
    },
}
